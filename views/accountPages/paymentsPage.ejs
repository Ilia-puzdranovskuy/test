<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<title>Віньковецький комунсервіс</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" >
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
<link href="/style/main.css" rel="stylesheet" type="text/css">
<body cla>
    <header>
        <%- include('../partials/header'); %>
    </header>
    <main>
        <div class=" persAc-main-container">
            <%-  include('../partials/persAcountNav'); %>
            <div class="persAc-main-sec-container">
                <div class="persAc-th-cont persAc-th-cont-main w-100" >
                    <div class="th-cont-title" >Платежі</div>
                    <div class="th-cont-body pers_ac_inf_block walp">
                        <table class="w-100 readings-table  table-pay-mob" >
                            <tr>
                            <th>Сума</th>
                            <th>Дата</th>
                            <th>Джерело</th>
                            <th>Послуга</th>
                            </tr>
                            
                                <% for(var i=0; i < payments.length; i++) { %>
                                    <tr>

                                        <% if(payments[i].sum != undefined){ %>
                                            <td><%= payments[i].sum%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>

                                        <% if(payments[i].date_time != undefined){ %>
                                            <td><%=moment( payments[i].date_time ).format("D-MM-YYYY")%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>

                                        <% if(payments[i].way != undefined){ %>
                                            <td><%= payments[i].way%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>
                                        <% if(payments[i].service != undefined){ %>
                                            <td><%= payments[i].service%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>

                                    </tr>
                                <% } %>
                        </table>
                    </div>
                </div>
            </div>

            <div class="persAc-main-sec-container">
                <div class="persAc-th-cont persAc-th-cont-main w-100" >
                    <div class="th-cont-title" >Нарахування</div>
                    <div class="th-cont-body pers_ac_inf_block">
                        <table class="w-100 readings-table all-readings-table" >
                            <tr>
                            <th>Послуга</th>
                            <th>Сума</th>
                            <th>Дата</th>
                            <th>Кількість споживачів</th>
                            <th>Попередні показники</th>
                            <th>Поточні показники</th>
                            <th>Споживання</th>
                       
                            </tr>
                            
                                <% for(var i=0; i < accurals.length; i++) { %>
                                    <tr>
                                        <% if(accurals[i].service != undefined){ %>
                                            <td><%= accurals[i].service%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>

                                        <% if(accurals[i].accural_sum != undefined){ %>
                                            <td><%= accurals[i].accural_sum%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>

                                        <% if(accurals[i].date != undefined){ %>
                                            <td><%=moment( accurals[i].date ).format("D-MM-YYYY")%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>
                                        
                                        <% if(accurals[i].num_people != undefined){ %>
                                            <td><%=accurals[i].num_people%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>
                                        <% if(accurals[i].previous_reading != undefined){ %>
                                            <td><%=accurals[i].previous_reading%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>
                                        <% if(accurals[i].curent_reading != undefined){ %>
                                            <td><%=accurals[i].curent_reading%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>


                                        <% if(accurals[i].consumption != undefined){ %>
                                            <td><%=accurals[i].consumption%></td>
                                        <% }else{ %>
                                            <td>Невідомо</td>
                                        <% } %>


                               

                                    </tr>
                                <% } %>
                        </table>
                    </div>
                </div>
            </div>
   
        </div>
        
    </main>

    <footer >
        <div style="position: absolute;bottom: 0;" class="w-100 footer-mobile">
            <%- include('../partials/footer'); %>
        </div>
    </footer>

    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js">
	</script> 

	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.min.js">
	</script>
    <script src="scripts/persAc.js"></script>

</body>
